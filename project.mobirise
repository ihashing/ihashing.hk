erlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-md-6 col-lg-6\">\n                \n                <h3 mbr-text class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .soc-wrapper\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">Empower SME with Blockchain Academy, Green Web3 Solutions &amp; dAPP Development.</h3>\n                <div class=\"soc-wrapper\">\n                    <div class=\"social-row\" mbr-if=\"showSocial\">\n                        <div class=\"soc-item\">\n                            <a href=\"https://www.facebook.com/ihashing.hk\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                            <a href=\"https://www.linkedin.com/company/ihashing/\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon-linkedin socicon\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                            <a href=\"https://www.instagram.com/ihashing.hk/\" target=\"_blank\">\n                   